            <td>
                @Html.DisplayFor(modelItem => item.Contraseña)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCargoNavigation.IdCargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEspecialidadNavigation.IdEspecialidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdHorarioNavigation.IdHorario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDoctor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDoctor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDoctor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
